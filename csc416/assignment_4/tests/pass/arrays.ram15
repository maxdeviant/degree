class Main {

    public static void main(String[] args) {
        print(new Arrays().run());
    }

}

class Arrays {

    public bool run() {
        int[] list;
        int i;
        int[] a;
        int[] b;
        int[] c;
        int[] d;
        int lengthA;

        list = new int[5];
        i = 0;

        while (i < 5) {
            list[i] = i + 1;
            i += 1;
        }

        a = list[1:3];
        b = list[2:];
        c = list[:3];
        d = list[:];

        println(a.length);
        println(b.length);
        println(c.length);
        println(d.length);

        lengthA = a.length;

        println(lengthA);

        return true;
    }

}
