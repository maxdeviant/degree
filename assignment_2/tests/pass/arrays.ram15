class Arrays {

    public static void main(String[] args) {
        int[] list = {1, 2, 3, 4, 5};

        int[] a = list[1:3]; // [2, 3, 4]
        int[] b = list[2:];  // [3, 4, 5]
        int[] c = list[:3];  // [1, 2, 3, 4]
        int[] d = list[:];   // [1, 2, 3, 4, 5]
    }

}
